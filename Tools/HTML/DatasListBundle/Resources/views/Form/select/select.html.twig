{% if selectedValue is not defined %}
    {%  set selectedValue = '' %}
{% endif %}
<select {% for key, attr in attrs %} {{ key }}="{{ attr }}" {% endfor %}>
{% if options is defined %}
    {% for option in options %}
        {{  include("DatasListBundle:Form/select:select_option.html.twig", {'value': option.value, 'text':option.text, 'selectedValue': selectedValue}) }}
    {% endfor %}
{% endif %}
{# non fonction #}
{% if datasList is defined %}
    {% for data in datasList %}
        {% set value = attribute(data, valuePropriety) %}
        {% set text = attribute(data, textPropriety)  %}
        {{  include("FormsBundle::select_option.html.twig", {'value': value, 'text': text, 'selectedValue': selectedValue}) }}
    {% endfor %}
{% endif %}

</select>